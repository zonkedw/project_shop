{
  "name": "food-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.3.0",
    "lucide-react": "^0.263.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "init-db": "cd backend && npm run init-db",
    "dev": "concurrently \"npm run backend\" \"npm start\"",
    "install:all": "npm install && cd backend && npm install",
    "clean": "rm -rf node_modules package-lock.json && cd backend && rm -rf node_modules package-lock.json",
    "clean:install": "npm run clean && npm run install:all"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
